<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.springtest.service.mapper.NeosMapper">
    <select id="compSelect"  resultType="java.util.HashMap">
        select	comp_name_kr, group_seq, comp_seq
        from t_co_comp
        where comp_seq = '1'
          and group_seq = 'klagoDev'
    </select>
</mapper>